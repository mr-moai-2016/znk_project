@def_quote [' ']

@@V config
my_libs_root = ['..']
template_dir = ['../../mkfsys/template']
install_dir = ['../../moai-v$(REL_VER)-$(PLATFORM)/cgis/easter']
special_exe_sfx = ['cgi']
runtime_install = ['false']
@@.

@@L include_paths_common
@{libZnk}
@{libRano}
@@.

@@L dependency_libs_common
rlib: @{libZnk}  Znk  $(DL_VER)
rlib: @{libRano} Rano $(DL_VER)
@@.

@@V info_vc
linking_libs_special = ['ws2_32 user32 gdi32']
@@.

@@V info_mingw
linking_libs_special = ['ws2_32 user32 gdi32']
@@.

@@V info_msys
linking_libs_special = ['ws2_32 user32 gdi32']
@@.

@@V info_linux
linking_libs_special = ['pthread dl stdc++']
@@.

@@V info_cygwin
linking_libs_special = ['pthread dl stdc++']
@@.

@@V info_android
compiler_option_special = ['-Wno-invalid-source-encoding']
@@.

@@L mkid_list
vc
mingw
msys
linux
cygwin
android
@@.

@@L product_list
exec easter main.c
exec easter_maintainer easter_maintainer.c
@@.

@@L rc_list
@@.

